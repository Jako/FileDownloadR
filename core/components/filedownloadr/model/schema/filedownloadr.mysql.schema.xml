<?xml version="1.0" encoding="UTF-8"?>
<model package="filedownloadr" baseClass="xPDOObject" platform="mysql" defaultEngine="InnoDB" phpdoc-package="filedownloadr" phpdoc-subpackage="model" version="1.1">
